  "bin": {
    "bun-decompile": "./src/index.ts"
  },
  "exports": {
    ".": "./src/lib/index.ts"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "bun",
    "decompile",
    "decompiler",
    "deminify",
    "reverse-engineering",
    "sourcemap",
    "ai",
    "claude",
    "openai"
  ],
  "author": "Jerred Shepherd",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shepherdjerred/monorepo.git",
    "directory": "packages/bun-decompile"
  },
  "homepage": "https://github.com/shepherdjerred/monorepo/tree/main/packages/bun-decompile#readme",
  "bugs": {
    "url": "https://github.com/shepherdjerred/monorepo/issues"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "scripts": {
    "build": "true",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@anthropic-ai/tokenizer": "^0.0.4",
