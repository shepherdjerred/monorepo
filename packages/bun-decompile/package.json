    "@babel/plugin-syntax-jsx": "^7.27.1",
    "@babel/plugin-syntax-typescript": "^7.27.1",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "openai": "^6.15.0",
    "tiktoken": "^1.0.22"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "bun-types": "^1.3.5",
    "typescript": "^5.7.2"
  }
