{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "noEmit": true,
    "declaration": false,
    "declarationMap": false,
    "allowJs": true,
    "verbatimModuleSyntax": false,
    "esModuleInterop": true,
