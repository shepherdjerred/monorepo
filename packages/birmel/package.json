{
  "name": "@shepherdjerred/birmel",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "start": "bunx prisma db push --skip-generate && bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "build": "true",
    "test": "bunx --env-file=.env.test prisma generate && bun --env-file=.env.test test",
    "typecheck": "prisma generate && tsc --noEmit",
    "lint": "eslint ."
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@prisma/client": "^6.0.0",
    "@sentry/node": "^10.32.1",
    "@voltagent/core": "^2.2.1",
    "@voltagent/libsql": "^2.0.3",
    "ai": "^6.0.3",
    "chrono-node": "^2.9.0",
    "cron-parser": "^5.4.0",
    "discord-player": "7.1.0",
    "discord-player-youtubei": "^1.5.0",
    "discord.js": "^14.25.1",
    "hono": "^4.11.1",
    "mediaplex": "^1.0.0",
    "playwright": "^1.57.0",
    "youtubei.js": "^16.0.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@shepherdjerred/eslint-config": "workspace:*",
    "@types/bun": "^1.3.5",
    "eslint": "^9.39.2",
    "prisma": "^6.0.0",
    "typescript": "^5.9.3"
  }
}
