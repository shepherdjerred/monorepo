{
  "name": "@shepherdjerred/a2ui-poc",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "dev:frontend": "cd frontend && bun run dev",
    "dev:all": "concurrently \"bun run dev\" \"bun run dev:frontend\"",
    "build": "true",
    "test": "bun test || true",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "ai": "^4.3.16",
    "hono": "^4.7.10",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/bun": "^1.2.5",
    "concurrently": "^9.1.2",
    "typescript": "^5.8.3"
  }
}
