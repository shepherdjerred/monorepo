{
  "name": "@shepherdjerred/birmel",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "start": "bunx prisma db push --skip-generate && bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "build": "true",
    "build:studio": "mastra build -d src/mastra",
    "test": "bun test",
    "typecheck": "prisma generate && tsc --noEmit",
    "lint": "eslint .",
    "studio:dev": "mastra dev -d src/mastra"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.0",
    "@discordjs/voice": "^0.18.0",
    "@mastra/core": "^1.0.0-beta.14",
    "@mastra/hono": "0.0.2-beta.9",
    "@mastra/libsql": "^1.0.0-beta.8",
    "@mastra/memory": "^1.0.0-beta.6",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@prisma/client": "^6.0.0",
    "@sentry/node": "^10.32.1",
    "ai": "^5.0.116",
    "discord-player": "7.1.0",
    "discord-player-youtubei": "^1.5.0",
    "discord.js": "^14.18.0",
    "hono": "^4.11.1",
    "libsodium-wrappers": "^0.7.15",
    "mediaplex": "^1.0.0",
    "openai": "^4.77.0",
    "sodium-native": "^5.0.10",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@types/bun": "^1.2.0",
    "eslint": "^9.30.0",
    "mastra": "1.0.0-beta.11",
    "prisma": "^6.0.0",
    "typescript": "^5.9.0",
    "typescript-eslint": "^8.35.0"
  }
}
